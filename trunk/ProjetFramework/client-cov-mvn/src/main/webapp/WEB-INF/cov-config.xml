	<form-beans>
		<form-bean name="paffichage"
			type="fr.afcepf.atod21.client.action.ActionPremiere" />
	</form-beans>
	<action-mappings>
		<action path="/affichage" type="fr.afcepf.atod21.client.action.ActionPremiereForm"
